    <Style x:Key="SliderStylePlayer" TargetType="Slider">
        <Setter Property="Maximum" Value="10"/>
        <Setter Property="Minimum" Value="0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Slider">
                    <Grid Background="Transparent">
                        <Grid x:Name="HorizontalTemplate">
                            <Rectangle x:Name="HorizontalTrack" Visibility="Collapsed" Fill="{TemplateBinding Background}" Height="8" IsHitTestVisible="False" Margin="0,22,48,50" Opacity="0.2"/>
                            <Rectangle x:Name="HorizontalFill" Height="8" IsHitTestVisible="False" Margin="0,22,0,50" Fill="#FFB63939">
                                <Rectangle.Clip>
                                    <RectangleGeometry Rect="-24, 0, 0, 8"/>
                                </Rectangle.Clip>
                            </Rectangle>
                            <Grid x:Name="HorizontalCenterElement" HorizontalAlignment="Left" Margin="-24,0,0,0">
                                <Grid.RenderTransform>
                                    <TranslateTransform/>
                                </Grid.RenderTransform>
                                <Grid Margin="0,0,0,-16">
                                    <Image Stretch="None" Source="Assets/Images/seekerThumb.png"/>
                                    <Rectangle Fill="Transparent"/>
                                </Grid>
                                <Grid x:Name="GrdPositionDisplay" Margin="5,-10,0,0">
                                    <TextBlock x:Name="TxtBlkPositionDisplayText" Text="{Binding Value,Converter={StaticResource DurationMillisecondsConverter},RelativeSource={RelativeSource TemplatedParent}}" Foreground="White" Margin="5" FontSize="16"/>
                                </Grid>
                            </Grid>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
